{
  "name": "media-server",
  "version": "beta",
  "description": "발행-구독을 이용한 영상 처리 서버 연동",
  "author": "",
  "private": "false",
  "license": "MIT",
  "scripts": {
    "rimraf": "./node_modules/rimraf/bin.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prestart": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "pm2 startOrReload ecosystem.config.json",
    "start:org": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:clear": "jest --clearCache"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.282.0",
    "@aws-sdk/client-sqs": "^3.282.0",
    "@aws-sdk/s3-request-presigner": "^3.282.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/typeorm": "^9.0.1",
    "chalk": "^4.1.2",
    "chardet": "^1.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cli-table3": "^0.6.2",
    "dayjs": "^1.11.7",
    "emoji-regex": "^10.2.1",
    "esbuild-jest": "^0.5.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.0",
    "mimic-fn": "^4.0.0",
    "module-alias": "^2.2.2",
    "npm": "^9.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "semaphore": "^1.1.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.21.0",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.3.9",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/module-alias": "^2.0.1",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "esbuild-jest"
    },
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "dist/",
      "backup/",
      "backup_integration/",
      "coverage/lcov-report",
      "test/"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@aws/(.*)": "<rootDir>/src/aws/$1",
      "^@task/(.*)": "<rootDir>/src/task/$1",
      "^@video/(.*)": "<rootDir>/src/video/$1",
      "^@chalk/(.*)": "<rootDir>/src/chalk/$1",
      "^@config/(.*)": "<rootDir>/src/config/$1",
      "^@ffmpeg/(.*)": "<rootDir>/src/ffmpeg/$1",
      "^@models/(.*)": "<rootDir>/src/models/$1",
      "^@semaphore/(.*)": "<rootDir>/src/semaphore/$1",
      "^@middlewares/(.*)": "<rootDir>/src/middlewares/$1"
    }
  },
  "_moduleAliases": {
    "@aws": "dist/aws",
    "@task": "dist/task",
    "@video": "dist/video",
    "@chalk": "dist/chalk",
    "@config": "dist/config",
    "@ffmpeg": "dist/ffmpeg",
    "@models": "dist/models",
    "@semaphore": "dist/semaphore",
    "@middlewares": "dist/middlewares"
  }
}
